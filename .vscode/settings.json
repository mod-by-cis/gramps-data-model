{
  "[css]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[html]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "apc.font.family": "FiraCode,  Fira Code",
  "apc.monospace.font.family": "FiraCode,  Fira Code",
  "breadcrumbs.enabled": false,
  "chat.editor.fontFamily": "FiraCode,  Fira Code",
  "cSpell.language": "en, pl",
  "cSpell.words": [
    "altova",
    "dtdinc",
    "dtdinst",
    "grampsxml",
    "graphml",
    "Jing",
    "przekonwertowano",
    "TDTD",
    "Trang",
    "TRNG",
    "versiongramps",
    "versiongrampsxml",
    "XSDDTD",
    "XSDRNG"
  ],
  "debug.console.fontFamily": "FiraCode,  Fira Code",
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "deno.codeLens.test": true,
  "deno.config": "code/deno.json",
  "deno.defaultTaskCommand": "run",
  "deno.enable": true,
  "deno.trace.server": "verbose",
  "editor.codeLensFontFamily": "FiraCode,  Fira Code",
  "editor.defaultFoldingRangeProvider": "denoland.vscode-deno",
  "editor.defaultFormatter": "denoland.vscode-deno",
  "editor.experimental.preferTreeSitter.ini": true,
  "editor.experimental.preferTreeSitter.typescript": true,
  "editor.experimental.treeSitterTelemetry": true,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.guides.indentation": true,
  "editor.inlayHints.fontFamily": "FiraCode,  Fira Code",
  "editor.inlineSuggest.fontFamily": "FiraCode,  Fira Code",
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "editor.multiCursorModifier": "alt",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": "entity.name.tag",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#3ab986"
        }
      },
      {
        "scope": "punctuation.definition.tag",
        "settings": {
          "foreground": "#a86c48"
        }
      },
      {
        "scope": "entity.other.attribute-name",
        "settings": {
          "fontStyle": "bold italic",
          "foreground": "#ffadaf"
        }
      },
      {
        "scope": "meta.tag.sgml.doctype.xml",
        "settings": {
          "foreground": "#FFF"
        }
      },
      {
        "scope": "text.xml",
        "settings": {
          "foreground": "#5f554e"
        }
      },
      {
        "scope": "meta.tag.xml",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#FFF"
        }
      },
      {
        "scope": "string",
        "settings": {
          "foreground": "#2898cb"
        }
      },
      {
        "scope": "keyword",
        "settings": {
          "foreground": "#dad41a"
        }
      },
      {
        "scope": "meta.tag.type.dtd",
        "settings": {
          "foreground": "#2898cb"
        }
      },
      {
        "scope": "keyword.other.data-type.dtd",
        "settings": {
          "fontStyle": "bold",
          "foreground": "#C93669"
        }
      },
      {
        "scope": "constant.language.dtd",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#B58900"
        }
      },
      {
        "scope": "comment.block.dtd",
        "settings": {
          "fontStyle": "italic",
          "foreground": "#5F554E"
        }
      }
    ]
  },
  "editor.wordWrap": "off",
  "explorer.confirmDragAndDrop": false,
  "files.associations": {
    "*.gpx": "xml",
    "*.gramps": "xml"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "gitflow.disableOnRepo": true,
  "ipynb.experimental.serialization": true,
  "markdownlint.config": {
    "MD033": {
      "allowed_elements": [
        "h1",
        "h2",
        "h3",
        "a",
        "ul",
        "li",
        "button",
        "span",
        "hr",
        "code",
        "table",
        "tr",
        "th",
        "td",
        "i",
        "b",
        "u",
        "br"
      ]
    }
  },
  "material-icon-theme.folders.associations": {
    "deno": "../code/deno.json" 
  },
  "notebook.consolidatedRunButton": true,
  "notebook.defaultFormatter": "denoland.vscode-deno",
  "notebook.diff.overviewRuler": true,
  "notebook.formatOnSave.enabled": true,
  "notebook.lineNumbers": "on",
  "notebook.output.fontFamily": "FiraCode,  Fira Code",
  "notebook.stickyScroll.enabled": true,
  "scm.inputFontFamily": "FiraCode,  Fira Code",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.fontFamily": "FiraCode,  Fira Code",
  "terminal.integrated.profiles.windows": {
    "UCRT64_MSYS2": {
      "args": [
        "-defterm",
        "-here",
        "-no-start",
        "-ucrt64"
      ],
      "path": "C:/msys64/msys2_shell.cmd"
    }
  },
  "typescript.tsdk": "deno:/builtin",
  "window.autoDetectColorScheme": true,
  "workbench.colorCustomizations": {
    "breadcrumb.background": "#2E2722",
    "chat.requestBackground": "#2e2722a0",
    "editor.background": "#2E2722",
    "editor.findMatchBackground": "#01022581",
    "editor.findMatchHighlightBackground": "#00000062",
    "editor.selectionBackground": "#ff3737b0",
    "editor.selectionHighlightBackground": "#c4c3c2a1",
    "editorGroupHeader.background": "#2E2722",
    "editorGutter.background": "#2E2722",
    "editorMarkerNavigation.background": "#2E2722",
    "editorPane.background": "#2E2722",
    "editorStickyScroll.background": "#2E2722",
    "multiDiffEditor.background": "#2E2722",
    "multiDiffEditor.headerBackground": "#2E2722",
    "notebook.background": "#2E2722",
    "panel.background": "#2E2722",
    "panelStickyScroll.background": "#2E2722"
  },
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editorAssociations": {
    "{git,gitlens,git-graph}:/**/*.{md,csv,svg}": "default",
    "*.svg": "default"
  },
  "workbench.preferredLightColorTheme": "Solarized Dark",
  "workbench.sideBar.location": "right",
  "workbench.tree.enableStickyScroll": false,
  "xml.symbols.maxItemsComputed": 500000
}
